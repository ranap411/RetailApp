<aura:component implements="flexipage:availableForAllPageTypes,force:hasRecordId,lightning:actionOverride" access="global">
    <aura:attribute type="string" name="recordId" default="" description="recordId"></aura:attribute>
    <aura:handler name="init" value="{!this}" action="{!c.myAction}"/>
    <c:enquiryPage recordId="{!v.recordId}"></c:enquiryPage>
</aura:component>